<aiml version="2.0" encoding="UTF-8">

	<category>
        <pattern>HI</pattern>
        <template>
				Welcome to UNSW. May I know your name please?
        </template>
    </category>
	<category>
        <pattern>HELLO</pattern>
        <template>
            Welcome to UNSW. May I know your name please?
        </template>
    </category>
	
	<category>
		<pattern>*</pattern>
			<that>Welcome to UNSW. May I know your name please?</that>
		<template>
			<think><set name="username"><star /></set></think>
			Nice to meet you, <get name="username"/>. How can I help you?
		</template>
	</category>
	<category>
		<pattern>MY NAME IS *</pattern>
		<template>
			<think><set name="username"><star /></set></think>
			Nice to meet you, <get name="username"/>. How can I help you?
		</template>
	</category>

	
	<category>
        <pattern>HOW ARE YOU</pattern>
        <template>
            <random>
				<li>I'm good. Thank you.</li>
                <li>I'm fine. How are you?</li>
            </random>
        </template>
    </category>
	
    <category>
        <pattern>WHO * YOU</pattern>
        <template>
            <random>
				<li>I am a AI Chatbot for your assistance. </li>
                <li>I am a robot!</li>
                <li>I am your assistance!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>* MORNING</pattern>
        <template>
            <random>
                <li>Good morning</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>* EVENING</pattern>
        <template>
            <random>
                <li>Good Evening</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>BYE</pattern>
        <template>
            <random>
                <li>Bye bye</li>
                <li>Goodbye</li>
                <li>bye~</li>
            </random>
        </template>
    </category>

</aiml>